.block2{ padding:40px; background:#fff;}
/* tFocus */
#tFocus{width:1000px;overflow:hidden;margin:0 auto;}
#tFocus-pic{width:1000px;height:430px;position:relative;}
#tFocus-pic li{width:338px;height:243px;position:absolute;left:0;top:0;filter:alpha(opacity:0);opacity:0;}
#tFocusBtn{width:1000px;height:86px;z-index:20;overflow:hidden; position:relative;margin-top:10px;}
#tFocus-btn{width:924px;height:86px;position:relative;overflow:hidden;margin:0 auto;}
#tFocus-btn ul{width:100000px;position:absolute;left:0;top:0;}
#tFocus-btn li{height:80px;width:188px;display:block;float:left;margin-right:4px; cursor:pointer; overflow:hidden;}
#tFocus-btn li.active{background-position:-448px -100px;}
#tFocus-btn li.active img{border:3px solid #859cc6;width:180px; height:74px;}
#tFocus-leftbtn,
#tFocus-rightbtn{width:30px;height:80px;position:absolute;top:0;text-indent:-9999px;}
#tFocus-leftbtn{ left:0; background:url(../schools/images/btn-left.png) no-repeat 50% 50% #bfc3cb;}
#tFocus-rightbtn{ right:0; background:url(../schools/images/btn-right.png) no-repeat 50% 50% #bfc3cb;}
#tFocus-leftbtn:hover,
#tFocus-rightbtn:hover{ background-color:#859cc6;}
#tFocus-leftbtn:active,
#tFocus-rightbtn:active{ background-color:#748cb7;}


.titbar{ text-align:center; padding:40px 0; position:relative;}
.titbar h3{ font-size:22px; color:#545e7c;}
.titbar span{ position:relative; background:url(../temp/images/b.png) repeat-x 0 50%; display:inline-block; padding:0 100px;}
.titbar em{ text-transform:uppercase; background:#fff; color:#999999; padding:0 20px; }


.wrap-3,.wrap-5,.wrap-7,.wrap-9{ background:#f7f8fa;}
.wrap-3 .titbar em,
.wrap-5 .titbar em,
.wrap-7 .titbar em,
.wrap-9 .titbar em{ text-transform:uppercase; background:#f7f8fa; color:#999999; padding:0 20px; }



.wrap-1{ background:#fff; padding-bottom:100px;}

.mql { position:relative;  width:1000px; margin:0 auto;}
.mql li{ float:left; width:333px; padding:20px 0; }
.mql .img{ float:left; width:100px; height:100px;}
.mql .text{ float:left; width:200px; height:120px; position:relative; margin-left:20px;}
.mql .t{ line-height:18px; color:#828386; max-height:36px; overflow:hidden; margin:10px 0;}
.mql .text .more_school{ font-size:12px;position:absolute; left:0; bottom:0;border:1px solid #d4d8e6; height:28px; line-height:28px; padding:0 16px; color:#53628c; background:#f5f7fa;}
.mql .text .more_school:hover{ background:#53628c; border:1px solid #53628c; color:#fff;}
.mql .text h3 a{color:#53628c; height:20px; line-height:20px; overflow:hidden; font-size:18px;}
.mql .text p a{ font-size:12px;color:#aeafb3; height:15px; line-height:15px; overflow:hidden;margin-top:3px; text-transform:uppercase;}



.wrap-2{ background:#f7f8fa; position:relative;margin-top:80px;}
.wrap-2 .title-bar{ font-size: 22px; color:#545e7c; width:230px; height:65px; background:#fff; border:1px solid #dfe0e1;position:absolute; top:-38px; left:50%;margin-left:-115px; text-align:center;}
.wrap-2 .title-bar h3{ font-size:22px; color:#545e7c; padding:10px 0 5px;}
.wrap-2 .title-bar p{ text-transform:uppercase;color:#999999;}
.wrap-2 .cons{ background:#f0f2f5; width:1000px; margin:0 auto;}
.wrap-2 .cons .left{ float:left; width:660px;padding-right:40px; background:#f7f8fa;padding-top:70px; min-height:400px;} 
.wrap-2 .cons .right{ float:right; width:300px;padding-top:70px;} 
.wrap-2 .cons .right .h3{ font-size:18px; color:#7181a9; text-align:center;}
.wrap-2 .cons .right .p{ font-size:14px; margin-bottom:20px; color:#999999; text-align:center;}
.wrap-2 .cons .right .tb{ padding:0 36px;}
.wrap-2 .cons .right .tb table{ width:100%;}
.wrap-2 .cons .right .tb th{ text-align:right; color:#888888; font-size:14px; width:65px;padding-right:10px; vertical-align:top;}
.wrap-2 .cons .right .tb input{ margin-bottom:10px; height:25px; width:140px; background:#fff; border:1px solid #dadada; border-radius:2px; box-shadow:0 1px 2px rgba(0,0,0,.1) inset; padding:0 10px;}
.wrap-2 .cons .right .tb textarea{ height:55px; font-size:12px; width:140px; background:#fff; border:1px solid #dadada; border-radius:2px; box-shadow:0 1px 2px rgba(0,0,0,.1) inset; padding:10px;}
.wrap-2 .cons .right .tb a{ margin:20px 0; display:inline-block; padding:0 15px; height:32px; line-height:32px; background:#cf102d; color:#fff;}
.wrap-2 .cons .right .tb a:hover{ background:#bb0b26;}
.wrap-2 .cons .item{ position:relative; padding-left:134px; min-height:176px; margin-bottom:30px;}
.wrap-2 .cons .item .img{ position:absolute; left:0;top:0;}
.wrap-2 .cons .item .top{ font-size:16px;  color:#666666; border-bottom:1px dashed #c2c8d4; margin-bottom:15px; padding-bottom:15px;}
.wrap-2 .cons .item .top span{ color:#556484;}
.wrap-2 .cons .item .top em{ color:#d0d3dd; font-size:12px; margin:0 12px;}
.wrap-2 .cons .item .bottom { position:relative; padding:0 32px; font-size:14px; line-height:24px; color:#666666;}
.wrap-2 .cons .item .bottom .e1{ width:18px; height:16px; background:url("../temp/images/d1.png") no-repeat; position:absolute; left:0;top:0;}
.wrap-2 .cons .item .bottom .e2{ width:18px; height:16px; background:url("../temp/images/d2.png") no-repeat; position:absolute; right:0;bottom:0;}


.video-index{ width:1000px; margin:0 auto;  position:relative;}
.video-index .list{ overflow:hidden; width:1000px; height:210px;}
.video-index .list ul{ padding:0 0 0;}
.video-index .list li{ float:left; width:240px;padding-right:20px; position:relative; height:190px;}
.video-index .list li a{ display:block;}
.video-index .list li a span{ color:#334169; display:block; text-align:center; line-height:20px; height:20px;margin-top:4px; overflow:hidden;}
.video-index .list li a em{ color:#5f5f5f; display:block; text-align:center; line-height:20px; font-size:12px; color:#616266;}
.video-index .list li .img{ position:relative; width:230px; height:124px;}
.video-index .list li .img img{ width:100%; height:auto;}
.video-index .list li .img .popVideo{ position:absolute; left:0;top:0; width:100%; height:100%; }
.video-index .list li .img .popVideo .i{ width:80px; height:80px; position:absolute; left:50%; top:50%; margin:-40px 0 0 -40px; background:url(../schools/images/btn.png) left bottom no-repeat;}
.video-index .list li a:hover .img .popVideo{  background:url(../schools/images/fil-bg.png);}
.video-index .list li a:hover .img .popVideo .i{ background-position:left top;}
.video-index .list li a:hover span{ color:#334169;}
.video-index .btn{ position:absolute; top:50%; margin-top:-60px; width:40px; height:40px;}
.video-index .btnLeft{ left:-50px; background:url(../schools/images/btn-dleft.png) no-repeat;}
.video-index .btnRight{ right:-50px; background:url(../schools/images/btn-dright.png) no-repeat;}
.video-index .btn:hover{ opacity:.8;}

