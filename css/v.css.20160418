.cob{ background:#fff; border:1px solid #e1e3e8; width:1000px; margin:0 auto; text-align:center; line-height:24px; padding:10px; font-size:14px; color:#666666;}

.highlights{ padding:45px 0;width:1000px; margin:0 auto;}

.highlights li{ font-size:14px; color:#666; float:left; width:160px; padding:20px; text-align:center; line-height:150%; min-height:210px;  transition:all .3s ease; border-radius:4px;}
.highlights li span{ display:block; width:126px; height:126px; overflow:hidden; margin:0 auto;}

.highlights li.s1 span{ background:url(../temp/images/a1.png) no-repeat 0 0;}
.highlights li.s2 span{ background:url(../temp/images/a2.png) no-repeat 0 0;}
.highlights li.s3 span{ background:url(../temp/images/a3.png) no-repeat 0 0;}
.highlights li.s4 span{ background:url(../temp/images/a4.png) no-repeat 0 0;}
.highlights li.s5 span{ background:url(../temp/images/a5.png) no-repeat 0 0;}
.highlights li h5{ font-size:18px; color:#3c3c77; margin:18px 0 10px;}
.highlights li img{transition:all 0.5s ease;}
.highlights li:hover img{transform: rotate(45deg);}
.highlights li:hover{ background:#5873a0;}
.highlights li:hover h5{ color:#fff;}
.highlights li:hover p{ color:#fff;}
.highlights li:hover span{ background-position:left bottom;}
.highlights li.s1:hover{ background:#6c7a96;}
.highlights li.s2:hover{ background:#7f8dab;}
.highlights li.s3:hover{ background:#9fb2c3;}
.highlights li.s4:hover{ background:#adc1d5;}
.highlights li.s5:hover{ background:#b4bfd4;}

