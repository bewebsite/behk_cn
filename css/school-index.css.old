.video-index .list li .img{ position:relative; width:260px; height:140px;}
.video-index .list li .img .popVideo{ position:absolute; left:0;top:0; width:100%; height:100%; }
.video-index .list li .img .popVideo .i{ width:80px; height:80px; position:absolute; left:50%; top:50%; margin:-40px 0 0 -40px; background:url(../schools/images/btn.png) left bottom no-repeat;}
.video-index .list li a:hover .img .popVideo{  background:url(../schools/images/fil-bg.png);}
.video-index .list li a:hover .img .popVideo .i{ background-position:left top;}
.video-index .list li a:hover span{ color:#334169;}
.video-index .btn{ position:absolute; top:50%; margin-top:-20px; width:40px; height:40px; display:none;}
.video-index .btnLeft{ left:0; background:url(../schools/images/btn-dleft.png) no-repeat;}
.video-index .btnRight{ right:0; background:url(../schools/images/btn-dright.png) no-repeat;}
.video-index .btn:hover{ opacity:.8;}

