.video-index{ width:1140px; margin:0 auto; background:#fff; margin-top:20px; padding:20px; position:relative;}
.video-index .top{ color:#4a5b7c; position:relative; height:32px; border-bottom:1px solid #cccccc;}
.video-index .top span{ position:absolute; left:0;top:0; font-size:18px; border-bottom:2px solid #4e638c; height:31px;}
.video-index .top a{font-size:12px; position:absolute; right:0;top:0; background:#f5f7fa;  padding:0 8px; height:22px; line-height:22px; color:#3d5699; border:1px solid #b9c2da;font-size:12px;}
.video-index .top a:hover{ background:#53628c; border:1px solid #53628c; color:#fff;}
.video-index .list{ overflow:hidden; width:1140px; height:210px;}
.video-index .list ul{ padding:20px 0 0;}
