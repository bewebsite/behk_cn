.school-houdong{ border-top:2px solid #334169;margin-top:20px; background:#fff;}
.school-houdong .bar{ background:#f5f7fa; height:50px; line-height:50px; position:relative; padding:0 20px; font-size:18px; color:#4a5b7c;}
.school-houdong .bar a{ position:absolute; right:20px;top:14px; color:#3d5699; border:1px solid #b3bdd7; padding:0 10px; height:22px; line-height:22px; font-size:12px;}
.school-houdong .bar a:hover{ background:#fff;}
.school-houdong .con { padding:20px 0 20px 20px;}
.school-houdong .con a{background:#ebeef7; width:415px; height:134px; float:left;margin:0 10px 10px 0; transition:all .3s ease;}
.school-houdong .con h3{ background:#53628c; border-bottom:1px solid #53628c; font-size:18px; height:49px; line-height:49px; padding:0 20px; overflow:hidden;transition:all .3s ease;}
.school-houdong .con .text{ color:#616266; padding:10px 0 0 20px; height:30px; line-height:30px;}
.school-houdong .con span{margin-right:20px;}
.school-houdong .con i img{margin-right:4px;}
.school-houdong .con a:hover{ background:#fafafa; box-shadow:0 0 8px rgba(0,0,0,.1)}
.school-houdong .con a:hover h3{background:#fafafa; color:#53628c; border-bottom:1px dotted #bfbfbf; margin:0 20px; padding:0;}

.news-con{  padding:20px 20px 0;}
.news-con li{ float:left;width:235px; padding:17px 20px; margin-left:-1px; margin-bottom:20px; border:1px solid #fff;border-left:1px dotted #dedee0; border-right:1px dotted #dedee0; }
.news-con li h3 a{ color:#53628c; font-size:18px; height:24px; line-height:24px; display:block; overflow:hidden;}
.news-con li .time{ color:#a0a3ab; font-size:12px;}
.news-con li .text{ margin-top:10px; color:#616266;}
.news-con li .text a{ color:#6d79a0; font-size:12px;}
.news-con li a:hover{ text-decoration:underline;}
.news-con li:hover{ box-shadow:0 0 10px rgba(0,0,0,.2); border:1px solid #dedee0; background:#fff;}
.news-con li:nth-child(1),
.news-con li:nth-child(4){border-left:1px solid #fff;}
.news-con li:nth-child(3),
.news-con li:nth-child(6){border-right:1px solid #fff;}

.mql-wrap{ width:1180px; margin:0 auto;}
.mql-wrap .bar{ height:48px; background:#334169; line-height:48px; position:relative; color:#fff;}
.mql-wrap .bar h3{ font-size:18px; padding-left:20px;}
