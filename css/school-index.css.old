.wrap{ font-size:14px; padding-bottom:40px;}
.slideBanner{ height:390px; overflow:hidden; position:relative;}
.slideBanner .imglist ul{ position:absolute; left:0;top:0;}
.slideBanner .imglist li{ float:left; width:880px; height:390px;}
.slideBanner .popBar{ position:absolute; width:100%; left:0; bottom:0; height:38px; line-height:38px; color:#fff; background:rgba(20,31,63,.6);*background:rgb(20,31,63);}
.slideBanner .popBar h3{ padding:0 20px;}
.slideBanner .anniu{ position:absolute; right:20px;top:0;}
.slideBanner .anniu a{ display:inline-block; width:40px; height:8px;background:rgba(124,124,124,.8);*background:rgb(124,124,124); margin-left:5px;}
.slideBanner .anniu a.cur{ background:rgba(255,255,255,.8);*background:rgb(255,255,255);}
.slideBanner .btn{font-size:12px; position:absolute; top:50%; margin-top:-20px; width:40px; height:40px;}
