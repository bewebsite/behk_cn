.search-school{ background:#fff; border-top:2px solid #334169;margin-top:20px; padding:10px;}
.search-school h3{ border-bottom:1px dotted #b9c3da; height:36px; line-height:30px;font-size:18px; margin-bottom:10px;}
.search-school input[type="text"]{ height:25px; border:1px solid #dadada; border-radius:2px; padding:0 10px; width:130px;}
.search-school select{height:25px; border:1px solid #dadada; border-radius:2px; padding:0 10px; width:130px;}
.search-school td{padding-bottom:8px;}
.search-school input[type="button"]{ padding:0 20px; color:#fff; height:32px; background:#cf102d; border:0; cursor:pointer;}
.search-school input[type="button"]:hover{ background:#ca0d2a;}
