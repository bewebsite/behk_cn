            $(document).ready(function(){
                var objStr = ".UpLayer";
                $(objStr).each(function(i){
                    $(this).click(function(){
                        $($(objStr+" dd")[i]).show();
                        $($(objStr+" dt")[i]).addClass("UpLayer02");
                    });
                    $(this).hover(function(){},function(){
                        $($(objStr+" dd")[i]).hide();
                        $($(objStr+" dt")[i]).removeClass("UpLayer02");
