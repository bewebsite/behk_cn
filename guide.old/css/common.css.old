@charset "utf-8";
/* CSS Document */

body{background:#fff;line-height:200%;color:#333333;}
a{color:#4c4c4c;}
a:hover{color:#000;}

.nav-main{position:relative;margin:auto;padding:20px 0 0 0;height:76px;width:1000px;}
.nav-menubox{padding-top:5px; float:right;}
.nav-menu{float:left;padding-top:5px;margin-right:5px;}
.nav-linkbox{display:block;width:93px;height:37px;line-height:37px;text-align:center;font-size:14px;}


.nav-item{margin-right:10px;color:#fff;}
.nav-item:hover{color:#005bac;}
.nav-tel{float:left;margin-left:10px;color:#49555e;}
