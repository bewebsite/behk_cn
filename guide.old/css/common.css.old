.popBg{ display:none; background:url(/guide/images/fil.png); position:fixed; left:0;top:0; width:100%; height:100%; z-index:1000;}
.popbor{background:url(/guide/images/fil.png); position:fixed; left:50%;top:50%; margin:-150px 0 0 -340px; z-index:1001; width:680px; height:300px;z-index:1001; }
