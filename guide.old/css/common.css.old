.nav-linkbox:hover{background:#217FC3;color:#fff;}

.vbanner{ position:relative; height:500px; position:relative; overflow:hidden;}
.vbanner .img{ position:absolute; left:50%;top:0;margin-left:-707px;}
.vbanner .text{ position:absolute; width:310px; margin:0 0 0 -500px; left:50%;top:363px; padding-left:690px;}
.vbanner .text .a{margin-top:40px;}
.vbanner .text .a a{ display:inline-block;  text-align:center; width:138px; height:36px; background:#fe0000;}
.vbanner .text .a a img{ vertical-align:middle;margin-top:8px;}
.vbanner .text .a .a1{margin-right:15px}
.vbanner .text .a .a2{ background:#0b2250;}
.vbanner .text .a a:hover{ opacity:.8;}
.vbanner .text .a a:active{ opacity:.9;}


.barNav,
.fxdNav{ background:#0b2250; height:46px; text-align:center; line-height:46px;}
.fxdNav a{ font-size:16px; color:#fff;}
.fxdNav a:hover{ color:#a5a9b5;}
.fxdNav em{ color:#707786; margin:0 10px;}

.about{ width:1000px; margin:0 auto; font-size:14px; padding-bottom:45px;}
.about td div{ padding:0 0 0 30px; text-align:justify;}
.titImg{ text-align:center; padding:40px 0 30px;}

.footer{ width:1000px; margin:0 auto; text-align:center;}
.footer .tips {height:50px; line-height:50px; color:#000;}
.footer .tips span{ color:#f00001;}
.footer .logoList{ height:50px; padding:25px 0; border:1px solid #e4e4e4;}
.footer .logoList img{ margin:0 10px;}

.footerNav{border-top:2px solid #0052ae;padding:10px 0 20px;margin-top:10px;}
.footerNav em{ margin:0 15px;}


.main{ margin:0 auto; width:1000px;}
.audience{ background:#ebedf0; padding:0 0 45px;}
.audience .audDl dl{ float:left;}

.audience .audDl dt{ transition:all .2s ease; float:left; width:80px; height:84px; text-align:center; line-height:84px; background:#4c5d7e; font-size:50px; letter-spacing:-3px; color:#fff; font-weight:700;}
.audience .audDl dd{ float:left; height:84px; width:240px; background:#fff; position:relative; overflow:hidden;}
.audience .audDl dd p{ padding:20px 0 0 16px; font-size:14px; color:#333333; height:64px;}
.audience .audDl dd div{ position:absolute; left:0;bottom:0; width:240px; transition:all .3s ease;}
.audience .audDl dd p.p1{ background:#5873a0; color:#fff;}
.audience .audDl .r{ float:right;}
.audience .audDl .c{margin-left:20px;}
.audience .audDl dl:hover dd div{ bottom:-84px;}

.elAud{ border:1px dotted #a0afc7; position:relative; margin:40px 0 0; padding:20px;}
.elAud .tps{ position:absolute; background:#ebedf0;font-size:14px; color:#4c5d7e; width:150px; text-align:center; height:16px; line-height:16px; left:50%; margin-left:-75px; top:-8px;}
.elAud .tps2{ text-align:center; margin-bottom:30px;}
.elAud .item{ overflow:hidden; float:left; width:300px; padding-left:100px; position:relative; margin:0 0 10px 40px; height:30px;}
.elAud .even{padding-left:130px;}
.elAud .item span{ position:absolute; width:86px; text-align:right; left:0;top:0; height:28px; line-height:28px; font-size:14px; color:#000;}
.elAud .even span{ position:absolute; width:120px; text-align:right; left:0;top:0; height:28px; line-height:28px;}
.elAud .item input[type="text"]{ width:280px; height:28px; line-height:28px\9; padding:0 10px; background:url(/guide/images/iconInput.png) left top repeat-x #fdfdfe; border-radius:3px; border:0;}
.elAud .item select{ padding:0 10px;}
.elAud .btn{ float:left;}
.elAud .btn a{ background:#f13122; margin-left:170px; color:#fff; height:40px; line-height:40px; padding:0 20px; display:inline-block; font-size:14px;}
.elAud .btn a:hover{ background:#e5291a;}
.elAud .btn a:active{ background:#de2213;}

.highlights{ background:#fff; padding:0 0 45px;}

.highlights li{ float:left; width:160px; padding:20px; text-align:center; line-height:150%; min-height:230px;  transition:all .3s ease; border-radius:4px;}
.highlights li span{ display:block; width:126px; height:126px; overflow:hidden; margin:0 auto;}

.highlights li.s1 span{ background:url(/guide/images/h1.png) no-repeat 0 0;}
.highlights li.s2 span{ background:url(/guide/images/h2.png) no-repeat 0 0;}
.highlights li.s3 span{ background:url(/guide/images/h3.png) no-repeat 0 0;}
.highlights li.s4 span{ background:url(/guide/images/h4.png) no-repeat 0 0;}
.highlights li.s5 span{ background:url(/guide/images/h5.png) no-repeat 0 0;}
.highlights li h5{ font-size:18px; color:#3c3c77; margin:18px 0 10px;}
.highlights li img{transition:all 0.5s ease;}
.highlights li:hover img{transform: rotate(45deg);}
.highlights li:hover{ background:#5873a0;}
.highlights li:hover h5{ color:#fff;}
.highlights li:hover p{ color:#fff;}
.highlights li:hover span{ background-position:left bottom;}
.highlights li.s1:hover{ background:#6c7a96;}
.highlights li.s2:hover{ background:#7f8dab;}
.highlights li.s3:hover{ background:#9fb2c3;}
.highlights li.s4:hover{ background:#adc1d5;}
.highlights li.s5:hover{ background:#b4bfd4;}


.schedule{ background:#ebedf0; padding:0 0 45px;}
.schedule .cons{ min-height:231px; background:#fff; width:448px; border:1px solid #c7d2e6; float:left; padding:20px; font-size:14px; line-height:30px; position:relative;}
.schedule .right{ float:right;}
.schedule .ctit{ text-transform:uppercase; padding-bottom:10px; margin-bottom:10px; border-bottom:1px solid #bdc6d7; font-size:18px; font-weight:700; color:#0b2250; padding-left:20px; background:url(/guide/images/icon-d.png) no-repeat 0 12px;}
.schedule .top span{ color:#black;}
.schedule p{ color:#000;}
.schedule .txt li{ position:relative; color:#0b2250; padding-left:30px;}
.schedule .txt li em{ width:16px; line-height:16px; text-align:center;  position:absolute; left:0;top:8px; background:#5873a0; color:#fff; }
.schedule .btn{ position:absolute; right:20px; bottom:20px; width:120px;}
.schedule .btn a{ display:inline-block; width:120px; height:36px; line-height:36px; text-align:center; color:#fff; background:#fe0000; margin-top:10px;}
.schedule .btn a.v2{ background:#0b2250;}
.schedule .btn a:hover{ opacity:.8;}
.schedule .btn a:active{ opacity:.9;}


.guests{ width:1000px; margin:0 auto; position:relative; margin-bottom:15px;}
.guests .tTxt{ position:absolute; width:260px; height:55px; top:-34px; left:50%;margin-left:-130px; text-align:center; background:#fff; border:1px solid #5873a0; padding-top:15px;}
.guests .img{ position:absolute; left:30px;top:66px;}
.guests .top,
.guests .bom{ padding-left:260px; font-size:14px;}
.guests .top{ background:#5873a0;padding-top:68px; color:#fff; height:127px;}
.guests .bom{ background:#e2e5ec; min-height:136px;padding-bottom:20px;}
.guests .top dl{ float:left;margin-right:60px;}
.guests .top dt{ margin-bottom:10px;}
.guests .top .dl1 h3{ font-size:24px; line-height:30px;}
.guests .top .dl2{ width:400px;}
.guests .top .dl2 p{ height:60px; overflow:hidden; line-height:21px;}
.guests .bom .tit{ background:url(/guide/images/rx.png) repeat-x left center; padding:10px 0 10px;margin-right:30px;}
.guests .bom .tit span{ background:#e2e5ec; padding-right:10px;}
.guests .bom li{ position:relative; color:#0b2250; padding-left:30px; line-height:30px;}
.guests .bom li em{ width:16px; line-height:16px; text-align:center;  position:absolute; left:0;top:8px; background:#5873a0; color:#fff; }

.guests2 .top{ background:#5873a0;padding-top:38px; color:#fff; min-height:127px;}
.guests2 .img{ position:absolute; left:30px;top:40px;}

#guests{padding-top:70px;}

.schools{ width:1000px; margin:0 auto 40px; position:relative;}
.schools .txt{ border:1px solid #0b2250; padding:20px;}
.schools .con{ border-left:1px dashed #d2d6df; margin:0 0 0 20px; padding-left:20px; font-size:14px; color:#0b2250;}

.sclList{padding-top:10px; width:1100px; overflow:hidden;}
.sclList .hidden{ width:1200px; margin-left:-30px;}
.sclList .item{ width:484px; height:475px; overflow:hidden; background:#0b2250; float:left; margin:20px 0 0 30px;}
.sclList .item:hover{ background:#233861;}
.sclList .item .left{ padding:3px; float:left; }
.sclList .item .left .top{ background:#fff; text-align:center; height:85px; padding-top:10px;}
.sclList .item .right{ float:left; width:244px; padding:30px 20px; color:#fff;font-size:14px; }
.sclList .item .right .top{ border-bottom:1px solid #7e8aa2; padding-bottom:10px; margin-bottom:10px;}
.sclList .item .right .top .tit1{ font-size:16px;} 
.sclList .item .right .top .tit1 a{ color:#fff;}
.sclList .item .right .top h4{ font-size:28px; line-height:30px;}
.sclList .item .right .top h4 a{ color:#fff}
.sclList .item .right .add{ font-size:14px; line-height:20px; margin-bottom:20px; height:40px; overflow:hidden;}
.sclList .item .right .txtFon td{ vertical-align:top; line-height:20px; }
.sclList .item .right .more{margin-top:10px;}
.sclList .item .right .more a{ color:#fff;}
.sclList .item .right .more a:hover{ text-decoration:underline;}
.sclList .item .right .aList{margin-top:20px;}
.sclList .item .right .aList a{float:left; font-size:16px; width:116px; height:36px; line-height:36px; text-align:center; color:#fff; background:#fe0000; margin-top:10px;}
.sclList .item .right .aList a.v2{ background:#fff; color:#0b2250; margin-left:10px;}
.sclList .item .right .aList a:hover{ opacity:.8;}
.sclList .item .right .aList a:active{ opacity:.9;}

.why{ background:#ebedf0; padding:0 0 45px;}
.be{ background:#fff; padding:0 0 45px;}
.book{ background:#ebedf0; padding:0 0 45px;}

.why .left{ float:left;}
